  <item id="ex-27884" cql="q-17" xml="topol-andel" en="en:topol-andel:0:390:2:w38 en:topol-andel:0:390:2:w37" cs="cs:topol-andel:0:397:2:w39" cst="doopravdy" use="content" comment="" predlemma="" predtag="" predverbtag="" ent="real for" cssent="K&#xE1;p , k&#x159;&#xE1;p &#x2026; nav&#xED;c si uv&#x11B;domil , &#x17E;e hehe &#x2026; pane doktore , von to toti&#x17E; &#x17E;&#xE1;dnej blud , &#x17E;&#xE1;dn&#xE1; , jak se to tentonoc &#x2026; psychogogick&#xE1; pseudohalucinace z drog a nez&#x159;&#xED;zen&#xFD;ho &#x17E;ivota neni , vono to doopravdy je ."/>
  <item id="ex-27891" cql="q-17" xml="Topol-Chladnou_zemi" en="en:Topol-Chladnou_zemi:0:372:5:w16 en:Topol-Chladnou_zemi:0:372:5:w15" cs="cs:Topol-Chladnou_zemi:0:392:1:w87" cst="doopravdy" use="content" predlemma="" predtag="" predverbtag="" ent="real for" cssent="Do Hlavn&#xED;ho stanu mohl za poplatek ka&#x17E;d&#xFD; , b&#x11B;hem ve&#x10D;ern&#xED;ch sezen&#xED; jsme ale byli jen sami mezi sebou , j&#xE1;dro na&#x161;eho spole&#x10D;enstv&#xED; , Kom&#xE9;nium &#x2026; z nov&#x11B; p&#x159;&#xED;choz&#xED;ch byly tyhle ve&#x10D;ery jen pro hleda&#x10D;e pry&#x10D;en , v&#x17E;dy jsme je mezi oby&#x10D;ejn&#xFD;mi turisty a zv&#x11B;davci poznali , S&#xE1;ra a Velk&#xE1; Lea neomyln&#x11B; a tak&#xE9; u&#x17E; i Rolf je poznal , to oni vodili lidi k Lebovi &#x2026; jen pro ne&#x161;&#x165;astn&#xE9; a chorobn&#xE9; m&#x11B;l Lebo opravdovou cenu , tady v dom&#x11B; Kom&#xE9;nia , v na&#x161;em squatu bylo v&#x161;echno doopravdy &#x2026;"/>
  <item id="ex-27896" cql="q-17" xml="Topol-Sestra" en="en:Topol-Sestra:0:227:8:w3" cs="cs:Topol-Sestra:0:219:8:w11" cst="doopravdy" use="content" comment="a&#x17E; m&#x11B;rov&#xE9;? " predlemma="" predtag="" predverbtag="" ent="actually" cssent="Asi byl teplej , proto&#x17E;e &#x17E;&#xE1;dnou z na&#x161;ich p&#x159;&#xED;telky&#x148; nikdy doopravdy neodt&#xE1;hl ."/>
  <item id="ex-26615" cql="q-17" xml="kundera-zert" en="en:kundera-zert:0:21:1:w15" cs="cs:kundera-zert:0:19:1:w15" cst="doopravdy" use="content" predlemma="" predtag="" predverbtag="" ent="actually" cssent="Pak dotyky ustaly a sly&#x161;el jsem , jak holi&#x10D;ka poodstupuje , jak te&#x10F; u&#x17E; doopravdy bere b&#x159;itvu do ruky , a tehdy jsem si &#x159;ekl ( nebo&#x165; my&#x161;lenky pokra&#x10D;ovaly ve sv&#xFD;ch hr&#xE1;ch ) , &#x17E;e mus&#xED;m uvid&#x11B;t , jak vlastn&#x11B; vypad&#xE1; dr&#x17E;itelka ( pozdvihovatelka ) m&#xE9; hlavy , m&#x16F;j n&#x11B;&#x17E;n&#xFD; vrah ."/>
  <item id="ex-25832" cql="q-17" xml="Douskova-Hrdy_Budzes" en="en:Douskova-Hrdy_Budzes:0:28:2:w5" cs="cs:Douskova-Hrdy_Budzes:0:24:2:w4" cst="doopravdy" use="content" predlemma="" predtag="" predverbtag="" ent="really" cssent="J&#xE1; se toti&#x17E; doopravdy nejmenuju Sou&#x10D;kov&#xE1; jako Ka&#x10D;enka , ale jmenuju se Freisteinov&#xE1; jako Karel Freistein , co byl n&#x11B;co jako m&#x16F;j t&#xE1;ta ."/>
  <item id="ex-28004" cql="q-17" xml="Vopenka-Moje_cesta" en="en:Vopenka-Moje_cesta:0:353:1:w10" cs="cs:Vopenka-Moje_cesta:0:343:1:w12" cst="doopravdy" use="content" predlemma="" predtag="" predverbtag="" ent="really" cssent="&#x201E; J&#xE1; si ale stejn&#x11B; mysl&#xED;m , &#x17E;e tihle to mysleli doopravdy , &#x201C; nenechal si tu p&#x159;edstavu vz&#xED;t ."/>
