  <item id="ex-25827" cql="q-15" xml="London-Tulak_po_hvezda" en="" cs="cs:London-Tulak_po_hvezda:0:414:3:w14" cst="bezpe&#x10D;n&#x11B;" use="certain" certainty="full" commfuntype="decl" commfunsubtype="domn&#x11B;nka s vysokou jistotou" scope="sent" pred="cs:London-Tulak_po_hvezda:0:414:3:w12 cs:London-Tulak_po_hvezda:0:414:3:w15" tfpos="neutral" sentpos="other" modalpersp="clear" evidencetype="reasoning" predlemma="b&#xFD;t v&#x11B;d&#x11B;t" predtag="VB-S---1P-AAI-- VpMS----R-AAI--" predverbtag="A----- VDA1SR" cssent="Av&#x161;ak moje t&#x11B;lo m&#x11B;lo tak bl&#xED;zko k &#xFA;pln&#xE9; smrti , &#x17E;e jsem naprosto bezpe&#x10D;n&#x11B; v&#x11B;d&#x11B;l , &#x17E;e rychl&#xFD;m soust&#x159;ed&#x11B;n&#xED;m v&#x16F;le na dosud &#x17E;iv&#xFD; kousek zbytku m&#xE9;ho t&#x11B;la p&#x159;estane existovat i moje t&#x11B;lo ."/>
  <item id="ex-25898" cql="q-15" xml="OCallaghan-nas_coney" en="" cs="cs:OCallaghan-nas_coney:0:298:16:w24" cst="bezpe&#x10D;n&#x11B;" use="content" predlemma="" predtag="" predverbtag="" cssent="Jeho vlastn&#xED; minulost mu p&#x159;ipadala daleko vzd&#xE1;len&#x11B;j&#x161;&#xED; , co&#x17E; prop&#x16F;j&#x10D;ovalo jejich vz&#xE1;jemn&#xE9;mu sd&#xED;len&#xED; v&#x11B;t&#x161;&#xED; d&#x16F;v&#x11B;rnost , a on v&#x17E;dycky po&#x10D;kal , dokud se bezpe&#x10D;n&#x11B; nezamknou v n&#x11B;jak&#xE9;m pokoji a sv&#x11B;t se nesmrskne jen na n&#x11B; dva , a a&#x17E; pot&#xE9; se otev&#x159;el ."/>
  <item id="ex-25374" cql="q-15" xml="Boyd-Neklid" en="" cs="cs:Boyd-Neklid:0:650:4:w18" cst="bezpe&#x10D;n&#x11B;" dictexample="1" use="content" predlemma="" predtag="" predverbtag="" cssent="A taky , hol&#x10D;i&#x10D;ko - mus&#xED;m k tob&#x11B; b&#xFD;t up&#x159;&#xED;mn&#xE1; - u&#x17E; se v tom dom&#x11B; nec&#xED;t&#xED;m bezpe&#x10D;n&#x11B; ."/>
